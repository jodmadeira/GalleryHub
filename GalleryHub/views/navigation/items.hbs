<h1>{{title}}</h1>
<hr>
<div class="row">
  {{#each items}}
    <div class="col-md-4 mb-5">
      <div class="card" style="width: 18rem;">
        <img src="{{this.itemSrc}}" class="card-img-top" >
        <div class="card-body">
          <h5 class="card-name">{{this.title}}</h5>
          <p class="card-text">{{this.description}}</p>
        </div>
      </div>
    </div>
  {{else}}
    <p>There are no items yet!</p>
  {{/each}}
  <hr />
</div>